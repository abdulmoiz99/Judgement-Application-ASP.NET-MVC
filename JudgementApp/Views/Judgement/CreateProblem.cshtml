@model List<JudgementApp.Models.Data>

@{
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>JudgementApplication</title>
    <link rel="stylesheet" href="~/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i">
    <link rel="stylesheet" href="~/assets/fonts/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/assets/css/Contact-Form-Clean.css">
    <link rel="stylesheet" href="~/assets/css/x-dropdown.css">
</head>
<body>
    <section class="contact-clean">
        <form method="post" style="text-align: center;">
            <h2 class="text-center">Create Problem</h2>
            <div>
                <div class="card shadow mb-4" id="Question-1">
                    <div class="card-header d-flex justify-content-between align-items-center">
                        <h6 class="text-primary fw-bold m-0">Question 1&nbsp;</h6>
                    </div>
                    <div class="card-body" style="text-align: left;">
                        <label class="form-label" style="font-style: normal;font-weight: bold;">will [P1] close [P2] by [P3] [P4]?</label>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P1</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                @foreach (var details in Model)
                                {
                                    if (details.Id == 0)
                                    {
                                        continue;
                                    }
                                    <option value=@details.Id>@details.SymbolName</option>
                                }
                            </select>
                        </div>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P2</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                <option value="13">higher</option>
                                <option value="14">lower</option>
                            </select>
                        </div>
                        <div style="width: 290px;"><label class="form-label" style="display: inline;padding-left: 4px;">P3</label><input class="form-control" type="text" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;"></div>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P4</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                <option value="12" selected="">today</option>
                                <option value="13">this week</option>
                                <option value="14">this month</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <div class="card shadow mb-4" id="Question-4">
                    <div class="card-header d-flex justify-content-between align-items-center">
                        <h6 class="text-primary fw-bold m-0">Question 2</h6>
                    </div>
                    <div class="card-body" style="text-align: left;">
                        <label class="form-label" style="font-style: normal;font-weight: bold;">will [P1] hit [P2] by [P3]?</label>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P1</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                @foreach (var details in Model)
                                {
                                    if (details.Id == 0)
                                    {
                                        continue;
                                    }
                                    <option value=@details.Id>@details.SymbolName</option>
                                }
                            </select>
                        </div>
                        <div style="width: 290px;"><label class="form-label" style="padding-left: 4px;">P2<input class="form-control" type="text" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;"></label></div>
                        <div style="width: 290px;">
                            <label class="form-label" style="display: inline;padding-left: 4px;">P3</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                <option value="12" selected="">12:00 noon</option>
                                <option value="13">12:30 pm</option>
                                <option value="14">01:00 pm</option>
                                <option value="15">01:30 pm</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <div class="card shadow mb-4" id="Question-3">
                    <div class="card-header d-flex justify-content-between align-items-center">
                        <h6 class="text-primary fw-bold m-0">Question 3</h6>
                    </div>
                    <div class="card-body" style="text-align: left;">
                        <label class="form-label"><strong>will [P1] close [P2] [P3] [P4]?</strong></label>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P1</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                @foreach (var details in Model)
                                {
                                    if (details.Id == 0)
                                    {
                                        continue;
                                    }
                                    <option value=@details.Id>@details.SymbolName</option>
                                }
                            </select>
                        </div>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P2</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                <option value="13">above</option>
                                <option value="14">Lower</option>
                            </select>
                        </div>
                        <div style="width: 290px;"><label class="form-label" style="display: inline;padding-left: 4px;">P3</label><input class="form-control" type="text" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;"></div>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">
                                P4<select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                    <option value="12" selected="">today</option>
                                    <option value="13">this week</option>
                                    <option value="14">this month</option>
                                </select>
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <div class="card shadow mb-4" id="Question-2">
                    <div class="card-header d-flex justify-content-between align-items-center">
                        <h6 class="text-primary fw-bold m-0">Question 4</h6>
                    </div>
                    <div class="card-body" style="text-align: left;">
                        <label class="form-label"><strong>will [P1] have a higher return in the AM or PM?</strong></label>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P1</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                @foreach (var details in Model)
                                {
                                    if (details.Id == 0)
                                    {
                                        continue;
                                    }
                                    <option value=@details.Id>@details.SymbolName</option>
                                }
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div>
                <div class="card shadow mb-4" id="Question-1">
                    <div class="card-header d-flex justify-content-between align-items-center">
                        <h6 class="text-primary fw-bold m-0">Question 5</h6>
                    </div>
                    <div class="card-body" style="text-align: left;">
                        <label class="form-label"><strong>will [P1] have a higher return before or after [P2]?</strong></label>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">P1</label><select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                @foreach (var details in Model)
                                {
                                    if (details.Id == 0)
                                    {
                                        continue;
                                    }
                                    <option value=@details.Id>@details.SymbolName</option>
                                }
                            </select>
                        </div>
                        <div style="width: 290px;">
                            <label class="form-label" style="padding-left: 4px;">
                                P2<select class="form-select" style="width: 250px;margin-top: 15px;display: inline;margin-left: 15px;">
                                    <option value="12" selected="">12:00 noon</option>
                                    <option value="13">12:30 pm</option>
                                    <option value="14">01:00 pm</option>
                                    <option value="15">01:30 pm</option>
                                </select>
                            </label>
                        </div>
                    </div>
                </div>
            </div><button class="btn btn-primary" type="button">Update</button>
        </form>
    </section>
    <script src="assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/js/theme.js"></script>
</body>
</html>
